{
  "name": "vue3-verify",
  "version": "0.0.0",
  "packageManager": "pnpm@10.23.0",
  "description": "Vue 3 verification component library",
  "type": "module",
  "keywords": [
    "vue3",
    "vue",
    "verification",
    "captcha",
    "slide",
    "puzzle",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/estjs/vue3-verify"
  },
  "author": "estjs",
  "files": [
    "dist"
  ],
  "main": "./dist/vue3-verify.umd.js",
  "module": "./dist/vue3-verify.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/vue3-verify.umd.js",
      "import": "./dist/vue3-verify.es.js"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "lint": "eslint src",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug"
  },
  "dependencies": {
    "vue": "^3.5.25"
  },
  "devDependencies": {
    "@estjs/eslint-config": "^2.1.5",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/ui": "^4.0.13",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.1",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.2.0",
    "pnpm": "^10.23.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.13",
    "vue-tsc": "^3.1.5"
  }
}
